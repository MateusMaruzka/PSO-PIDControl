import scipy.signal
import numpy as np
import matplotlib.pyplot as plt

P = scipy.signal.TransferFunction([1],[1,1])

t,y = scipy.signal.step(P)

plt.plot(t,y, 'k--')
plt.plot(t[14:],y[:len(y)-14])